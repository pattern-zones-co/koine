{
	"name": "@patternzones/koine-sdk",
	"version": "2.3.0",
	"description": "TypeScript SDK for Koine gateway",
	"repository": {
		"type": "git",
		"url": "https://github.com/pattern-zones-co/koine.git",
		"directory": "packages/sdks/typescript"
	},
	"homepage": "https://github.com/pattern-zones-co/koine",
	"bugs": "https://github.com/pattern-zones-co/koine/issues",
	"author": "Pattern Zones Co",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"lint": "biome check .",
		"example:hello": "bun --env-file=../../../.env run examples/hello.ts",
		"example:stream": "bun --env-file=../../../.env run examples/stream.ts",
		"example:stream-object": "bun --env-file=../../../.env run examples/stream-object.ts",
		"example:recipe": "bun --env-file=../../../.env run examples/extract-recipe.ts",
		"example:conversation": "bun --env-file=../../../.env run examples/conversation.ts"
	},
	"dependencies": {
		"zod": "3.25.76",
		"zod-to-json-schema": "3.24.5"
	},
	"devDependencies": {
		"@types/node": "25.0.2",
		"@vitest/coverage-v8": "3.2.4",
		"typescript": "5.9.3",
		"vitest": "3.2.4"
	},
	"engines": {
		"node": "22.21.1",
		"bun": "1.3.3"
	},
	"keywords": ["claude", "claude-code", "anthropic", "ai", "sdk", "gateway"],
	"license": "AGPL-3.0-only OR LicenseRef-Commercial",
	"files": ["dist", "README.md"]
}
